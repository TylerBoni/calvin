{
  "name": "ai-calendar-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "An intelligent calendar application that understands natural language event descriptions",
  "scripts": {
    "dev": "vite",
    "dev:android": "npx cap open android",
    "dev:ios": "npx cap open ios",
    "build": "vite build",
    "build:cap": "vite build && npx cap sync",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx src/lib/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push:pg",
    "setup:rls": "tsx scripts/setup-rls.ts",
    "setup:rls-simple": "tsx scripts/setup-rls-simple.ts"
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.2",
    "@lucide/svelte": "^0.515.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/vite": "^4.0.0-alpha.15",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^20.11.17",
    "bits-ui": "^2.8.6",
    "drizzle-kit": "^0.20.14",
    "postgres": "^3.4.7",
    "svelte": "^5.35.5",
    "svelte-check": "^4.2.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.0-alpha.15",
    "tsx": "^4.7.1",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.2"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@supabase/supabase-js": "^2.52.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.30.8",
    "openai": "^4.28.4",
    "pg": "^8.11.3",
    "tailwind-merge": "^3.3.1"
  }
}
